{
  "name": "",
  "private": true,
  "scripts": {
    "test": "nyc --cache --require babel-register tap test/math.js",
    "coverage": "rimraf ./coverage && nyc --require babel-register tap test/math.js --coverage-report=lcov",
    "compile": "rimraf ./dist && rimraf ./test-dist && babel ./src --out-dir ./dist && babel ./test --out-dir ./test-dist",
    "dev": "nodemon -q -x 'npm test'",
    "repl": "babel-node ./repl.js"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-preset-es2015": "6.5.0",
    "babel-register": "6.5.2",
    "babel-runtime": "6.5.0",
    "nodemon": "^1.9.1",
    "nyc": "^5.6.0",
    "rimraf": "^2.5.2",
    "tap": "^5.7.0"
  },
  "nyc": {
    "exclude": [
      "test",
      "test{,-*}.js",
      "test-dist"
    ],
    "sourceMaps": "both"
  }
}
